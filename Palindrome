// A C/C++ Program to generate OTP (One Time Password)

#include<bits/stdc++.h>
using namespace std;


bool palindrome(string s,int st,int e)
{
    if(s[st]!=s[e]){
        return 0;
    }
    if(st>=e){
    return 1;
    }
    return palindrome(s,st+1,e-1);
}


int main()
{

    
    string a="asdfdsa";
    cout<<palindrome(a,0,a.length()-1);
    
	
	return(0);
}
